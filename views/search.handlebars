<div class="container">
  {{#with charRender }}
  <h1 class="white-text">Search Character</h1>
  <div class="row">
    <form>
    <div class="input-field col s12">
      <select class="browser-default" id="searchchar">
        <option value="" disabled selected>Select Character</option>
        {{#each characters}}
        <option value={{id}}>{{first_name}} {{last_name}}</option>
        {{/each}}
        <label>Select Character</label>
      </select>
    </div>
    <a class="btn col s12" id="searchBtn"><i class="material-icon center">Search</i></a>
    </form>
  </div>
  {{!-- <a class="btn-large col s12" id="searchBtn"><i class="material-icon center">Search</i></a> --}}
  {{/with}}
  {{#if mostRecent }}
    {{#each mostRecent}}
      {{> character }}
    {{/each}}
  {{/if}}
</div>



